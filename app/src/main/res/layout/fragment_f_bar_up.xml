<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="layout.FBarUp">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="left">

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ibPreferenciasGames"
            android:background="@mipmap/ic_settings" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ibPreferencias"
            android:background="@mipmap/ic_settings"
            android:layout_toRightOf="@+id/ibPreferenciasGames" />

        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ibArcadeMode"
            android:background="@mipmap/ic_step"
            android:layout_toRightOf="@+id/ibPreferencias" />
    </RelativeLayout>

</FrameLayout>
